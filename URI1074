#include<iostream>
using namespace std;
int main()
{
    int value, n, i;
    cin>>n;

    for(i=0; i<n; i++)
    {
        cin>>value;
        if(value==0)
            cout<<"NULL"<<endl;

        else if(value>0)
        {
            if(value%2==0)
                cout<<"EVEN POSITIVE"<<endl;
            else
            cout<<"ODD POSITIVE"<<endl;
        }

        else
        {
            if(value%2!=0)
                cout<<"ODD NEGATIVE"<<endl;
            else
                cout<<"EVEN NEGATIVE"<<endl;
        }
    }

    return 0;
}
