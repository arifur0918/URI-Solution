#include <iostream>
using namespace std;
int main()
{
    int x, a, i, In = 0, Out = 0;
    cin >> x;
    for(i = 0; i < x; i++)
    {
        cin >> a;
        if(a >= 10 && a <= 20)
            In++;
        else
            Out++;
    }
    cout << In << " in\n";
    cout << Out << " out\n";
    return 0;
}
