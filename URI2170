#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    double x,y;
    int i=1;
    while(cin>>x>>y)
    {
    double A,B;
    B=y-x;
    
    A=(B*100)/x*1.00;
    cout<<"Projeto "<<i<<":"<<endl<<"Percentual dos juros da aplicacao: ";
    printf("%.2lf ",A);
    cout<<"%";
    cout<<endl<<endl;
    i++;
    }
    return 0;
}
