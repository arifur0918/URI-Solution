#include<stdio.h>

int main()
{
    long long int d1=1, d2=1, a, b, i, j;
    while(scanf("%lld %lld",&a,&b)!=EOF)
    {
        for(i=1; i<=a; i++)
        {
            d1*=i;
        }
        for(j=1; j<=b; j++)
        {
            d2*=j;
        }
        printf("%lld\n",d1+d2);
        d1=d2=1;
    }
    return 0;
}
